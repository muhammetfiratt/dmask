{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/styles.css",
      "dest": "/api/static/styles.css"
    },
    {
      "src": "/script.js",
      "dest": "/api/static/script.js"
    },
    {
      "src": "/index.html",
      "dest": "/api/static/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
